.log-fixed {
    position: fixed;
    background: #333;
    padding: 5px;
    right: 10px;
    bottom: 10px;
    color: #fff;
    font-size: 18px;
}

.screen {
    float: left;
    background: #f3f3f3;
    width: 100%;
    overflow: auto;
    
    &.mobile {
        width: 320px;
        height: 568px;
    }
}

.fields {}

.actions {
    width: 100%;
    overflow: hidden;
    margin: 5px 0;
    .button {
        padding: 10px;
        float: left;
        border: 1px solid gray;
        margin-right: 5px;
        &:hover {
            background: #1565C0;
            color: #fff;
            cursor: pointer;
        }
    }
}

.list {
    .item {
        padding: 2px;
        border: 1px solid #BDBDBD;
        margin-bottom: 1px;
    }
}


.fields {

    input {
        padding: 4px 6px;
        border: 1px solid #333; 
        //background: none;
        outline: none;
        margin: 1px;
        font-size: 13px;
        font-family: "Lato", sans-serif;
    }

}

.icon {
    width: 20px;
    height: 20px;
    &.small {
        width: 10px;
        height: 10px;
    }
}

.action-events {
    width: 100%;
    text-align: center;
    bottom: 10px;
    position: fixed;
}

.button {
    padding: 5px;
    width: 45px;
    height: 45px;
    border: none;
    background: gray;
    border-radius: 50%;
}

// #root-menu
.root-menu {
    width: 50px;
    height: 100%;
    position: fixed;
    background: #333742;
    color: #fff;
    
    &.close {
        left: -100vw;
    }
    &.open {
        left: 0;
    }
    .root-menu-item {
        width: 100%;
        padding: 15px;
        font-size: 15px;
        font-weight: 300;
        box-sizing: border-box;
    }

    .root-item-logo {
        background: #414550;
        border-bottom: 1px solid #333;
        
        font-size: 20px;
        height: 50px;
    }
}

// #root-status-bar
.root-status-bar {
    width: calc(100vw - 70px);
    height: 50px;
    max-height: 50px;
    position: absolute;
    top: 0;
    margin-left: 50px;
    padding: 10px;
    box-sizing: border-box;
    
    background: #fff;
    color: #333;
    border-bottom: 1px solid #E5EBEE;
    
    .root-status--title {
        float: left;
        width: 40%;
        font-size: 20px;
        box-sizing: border-box;
        height: 34px;
        line-height: 34px;
    }

    .root-status-bar--actions {
        float: right;
        width: 60%;

        .root-status-bar-item {
            float: right;
            height: 34px;
            width: 34px;
            text-align: center;
            line-height: 34px;
            font-size: 14px;
            &.automatic {
                width: auto;
                padding: 0 10px;
            }
        }

        .avatar {
            width: 34px;
            height: 34px;
            font-size: 18px;
            line-height: 34px;
        }
    }
}


// #root-contents
.root-contents {
    width: calc(100% - 50px);
    max-width: calc(100% - 50px);
    height: auto;
    position: absolute;
    top: 51px;
    overflow: hidden;
    
    margin-left: 50px;
    background: #fff;
    color: #333;
}

// #panel-component
.panel-component {
    float: left;
    // padding: 15px;
    box-sizing: border-box;

    &.panel-graph {
        width: calc(100% - 300px);
        background: #BAC9D1;
    }
    &.panel-history {
        width: 300px;
        background: #fff;
        .panel-component-title {
            border: none;
            font-size: 13px;
            text-transform: uppercase;
        }
    }

    .panel-component-title {
        font-size: 15px;
        width: 100%;
        border-bottom: 1px solid #A6B5BD;
        padding: 0 0 10px 0;
        margin: 15px;

    }
    .panel-component-status {
        height: 35px;
        padding: 20px 0;
        margin: 0 15px;
        .panel-component-status--summary {
            float: left;
            background: #fff;
            box-shadow: 0 0 20px;
            
            .panel-component-status--summary-item {
                float: left;
                border-left: 1px solid #DDE6EB;
                padding: 10px;
                &:first-child {
                    border: none;
                }
            }
        }
        
        .panel-component-status--filter {
            float: right;
            background: #fff;
            border-left: 1px solid #DDE6EB;
            .panel-component-status--filter-item {
                float: left;
                color: #333;
                text-align: center;
                padding: 10px;
                font-size: 14px;
                &.active {
                    background: #05AAE8;
                    color: #fff;
                }
            }
        }
    }
    .panel-component-graph {
        margin: 15px;
        height: 400px;
        
        canvas {
            width: 100%;
            height: 100%;
        }
    }

    .panel-component-body {
        padding: 10px;
    }

    .panel-component-acctions {
        padding: 15px;
    }
} 

.history-item {
    width: 100%;
    padding: 10px 10px 10px 20px;
    margin-left: 10px;
    font-size: 14px;
    border-left: 1px solid #A6B5BD;

    &::before {
        content: "ïª";
        position: absolute;
        margin-left: -30px;
        font-size: 22px;
        height: auto;
        margin-top: -3px;
        color: #62ABD2;
        background: #fff;
        font-family: FontAwesome;
    }
}

.transactions-history-component {
    width: 100%;
    background: #fff;
    float: left; 
}


// #user-menu
.user-menu {
    width: 150px;
    height: auto;
    color: #333;
    background: #f3f3f3;
    position: absolute;
    right: -100vw;

    &.close {
        right: -100vw;
    }
    &.open {
        right: 0;
    }

    .user-menu-item {
        width: 100%;
        padding: 15px;
        font-size: 15px;
        font-weight: 300;
        box-sizing: border-box;
    }
}

// #avatar
.avatar {
    width: 40px;
    height: 40px;
    background: gray;
    border-radius: 40px;
    line-height: 40px;
    text-align: center;
}


/* -- Material Design Table style -------------- */

// Variables
// ---------------------
$table-header-font-weight:      400;
$table-header-font-color:       #757575;

$table-cell-padding:            1.6rem;
$table-condensed-cell-padding:  $table-cell-padding/2;


$table-bg:                      #fff;
$table-bg-accent:               #f5f5f5;
$table-bg-hover:                rgba(0,0,0,.12);
$table-bg-active:               $table-bg-hover;
$table-border-color:            #e0e0e0;


// Mixins
// -----------------
// //.transition($transition) {
//   -webkit-transition: $transition;
//        -o-transition: $transition;
//           transition: $transition;
// }

// Tables
//
// -----------------

// Baseline styles
.table {
  width: 100%;
  max-width: 100%;
  margin-bottom: 2rem;
  background-color: $table-bg;
  > thead,
  > tbody,
  > tfoot {
    > tr {
      //.transition(all .3s ease);
      > th,
      > td {
        text-align: left;
        padding: $table-cell-padding;
        vertical-align: top;
        border-top: 0;
        //.transition(all .3s ease);
      }
    }
  }
  > thead > tr > th {
    font-weight: $table-header-font-weight;
    color: $table-header-font-color;
    vertical-align: bottom;
    border-bottom: 1px solid rgba(0,0,0,.12);
  }
  > caption + thead,
  > colgroup + thead,
  > thead:first-child {
    > tr:first-child {
      > th,
      > td {
        border-top: 0;
      }
    }
  }
  > tbody + tbody {
    border-top: 1px solid rgba(0,0,0,.12);
  }

  // Nesting
  .table {
    background-color: $table-bg;
  }

  // Remove border
  .no-border {
    border: 0;
  }
}

// Condensed table w/ half padding
.table-condensed {
  > thead,
  > tbody,
  > tfoot {
    > tr {
      > th,
      > td {
        padding: $table-condensed-cell-padding;
      }
    }
  }
}


// Bordered version
//
// Add horizontal borders between columns.
.table-bordered {
  border: 0;
  > thead,
  > tbody,
  > tfoot {
    > tr {
      > th,
      > td {
        border: 0;
        border-bottom: 1px solid $table-border-color;
      }
    }
  }
  > thead > tr {
    > th,
    > td {
      border-bottom-width: 2px;
    }
  }
}


// Zebra-striping
//
// Default zebra-stripe styles (alternating gray and transparent backgrounds)
.table-striped {
  > tbody > tr:nth-child(odd) {
    > td,
    > th {
      background-color: $table-bg-accent;
    }
  }
}

// Hover effect
//
.table-hover {
  > tbody > tr:hover {
    > td,
    > th {
      background-color: $table-bg-hover;
    }
  }
}

// Responsive tables (vertical)
//
// Wrap your tables in `.table-responsive-vertical` and we'll make them mobile friendly
// by vertical table-cell display. Only applies <768px. Everything above that will display normally.
// For correct display you must add 'data-title' to each 'td'
.table-responsive-vertical {

  @media screen and (max-width: 768px) {

    // Tighten up spacing
    > .table {
      margin-bottom: 0;
      background-color: transparent;
      > thead,
      > tfoot {
        display: none;
      }

      > tbody {
        display: block;

        > tr {
          display: block;
          border: 1px solid $table-border-color;
          border-radius: 2px;
          margin-bottom: $table-cell-padding;

          > td {
            background-color: $table-bg;
            display: block;
            vertical-align: middle;
            text-align: right;
          }
          > td[data-title]:before {
            content: attr(data-title);
            float: left;
            font-size: inherit;
            font-weight: $table-header-font-weight;
            color: $table-header-font-color;
          }
        }
      }
    }
    
    // Special overrides for shadows
    &.shadow-z-1 {
      -webkit-box-shadow: none;
      -moz-box-shadow: none;
      box-shadow: none;
      > .table > tbody > tr {
        border: none;
        //.shadow-z-1();
      }
    }

    // Special overrides for the bordered tables
    > .table-bordered {
      border: 0;

      // Nuke the appropriate borders so that the parent can handle them
      > tbody {
        > tr {
          > td {
            border: 0;
            border-bottom: 1px solid $table-border-color;
          }
          > td:last-child {
            border-bottom: 0;
          }
        }
      }
    }

    // Special overrides for the striped tables
    > .table-striped {
      > tbody > tr > td,
      > tbody > tr:nth-child(odd) {
          background-color: $table-bg;
      }
      > tbody > tr > td:nth-child(odd) {
          background-color: $table-bg-accent;
      }
    }

    // Special overrides for hover tables
    > .table-hover {
      > tbody {
        > tr:hover > td,
        > tr:hover {
          background-color: $table-bg;
        }
        > tr > td:hover {
          background-color: $table-bg-hover;
        }
      }
    }
  }
}


